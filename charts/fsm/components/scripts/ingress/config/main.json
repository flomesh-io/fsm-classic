{
  "http": {
    "enabled": true,
    "listen": 8000
  },

  "tls": {
    "enabled": false,
    "listen": 8443,
    "mTLS": false,
    "certificate": {}
  },

  "sslPassthrough": {
    "enabled": false,
    "upstreamPort": 443
  },

  "plugins": [
    "plugins/reject-http.js",
    "plugins/router.js",
    "plugins/logging.js",
    "plugins/metrics.js",
    "plugins/balancer.js",
    "plugins/default.js"
  ],

  "logging": {
    "url": "http://localhost:8123/ping",
    "headers": {},
    "token": "",
    "batch": {
      "size": 1000,
      "separator": "\n"
    }
  }
}